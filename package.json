    "@parcel/packager-html": "^2.15.2",
    "@parcel/packager-js": "^2.15.2",
    "@parcel/transformer-css": "^2.15.2",
    "@parcel/transformer-html": "^2.15.2",
    "@parcel/transformer-js": "^2.15.2",
    "parcel": "^2.15.2"
